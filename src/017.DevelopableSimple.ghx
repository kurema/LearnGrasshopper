    var curve = cf.GetCurve(300);
    curve.Domain = new Interval(-1, 1);
    var g3 = new Graph3d((u,v) =&gt; {
      var　tan = curve.CurvatureAt(u);
      var poi = curve.PointAt(u);
      return poi + tan * v;
      });
    g3.vInterval = new Interval(0, 3);
    g3.BakeSurface(RhinoDocument, 100, 100);
    A = curve;</item>
