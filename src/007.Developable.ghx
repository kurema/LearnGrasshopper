                    <item name="ScriptSource" type_name="gh_string" type_code="10">    var points = new List&lt;Point3d&gt;();

    Curve cv;

    var cvpts = new List&lt;Point3d&gt;();
    for(double u = 0;u &lt; Math.PI * 3;u += 0.3){
      cvpts.Add(new Point3d(Math.Sin(u), Math.Cos(u), u));
    }
    cv = Rhino.Geometry.NurbsCurve.CreateInterpolatedCurve(cvpts, 3);

    cv.Domain = new Interval(0, 1);
    int cnt = 100;
    int vcnt = 100;
    double vlen = 10;

    for(int i = 0;i &lt; cnt;i++){
      Vector3d tan = cv.TangentAt((double) i / (double) cnt);
      Point3d org = cv.PointAt((double) i / (double) cnt);
      //vlen = tan.X == 0 ? 10 : (20 - org.X) / tan.X;
      for(int j = 0;j &lt; vcnt;j++){
        points.Add(org + tan * vlen * (double) j / (double) vcnt);
      }
    }

    A = cv;
    A = Rhino.Geometry.NurbsSurface.CreateFromPoints(points.ToArray(), cnt, vcnt, 3, 3);</item>
